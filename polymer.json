{
  "entrypoint": "index.html",
  "shell": "src/workout-app.html",
  "fragments": [
    "src/workout-home.html",
    "src/workout-plans.html",
    "src/workout-stats.html",
    "src/workout-exercises.html",
    "src/workout-settings.html",
    "src/workout-404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
